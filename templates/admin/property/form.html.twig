{{ form_start(form) }}
<div class="row">
    <div class="col-md-6"> {{ form_row(form.titre) }}</div>
    <div class="col-md-6"> {{ form_row(form.prix) }}</div>
</div>
<div class="row">
    <div class="col-md-3"> {{ form_row(form.nbrechambre) }}</div>
    <div class="col-md-3"> {{ form_row(form.etage) }}</div>
    
</div>
<div class="row">
    <div class="col-md-3">  {{ form_row(form.image) }}</div>
    <div class="col-md-4">  {{ form_row(form.adresse) }}</div>
    <div class="col-md-4">  {{ form_row(form.validation) }}</div>

</div>
{{ form_rest(form) }}
<button class="btn btn-primary">{{ button|default('Enregistrer') }}</button>
{{ form_end(form) }}